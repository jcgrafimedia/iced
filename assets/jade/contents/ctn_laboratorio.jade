extends ../layouts/layout
block content
	section(id="container_laboratorio" , class="col-md-12")
		div(class="row")
			h3 RESERVA DE LABORATORIO
			div(class="container_article")
					div
						img(src="{{ ruta_base }}assets/img/laboratorio.jpg", alt="")
